{
    "compilerOptions": {
        "module": "commonjs",
        "target": "es2015",
        "declaration": true,
        "removeComments": true,
        "outDir": "./dist"
    },
    "include": [
        "*.ts"
    ],
    "typedocOptions": {
        "entryPoints": [
            "index.ts"
        ],
        "out": "docs",
        "includes": "./includes",
        "excludePrivate": true,
        "excludeInternal": true,
        "excludeProtected": true,
    }
}